;; widgets/calendar/calendar.yuck

;; Widgets

; Main Widget
(defwidget cal []
  (box
    :class "calendar"
  (calendar)
))


;; Main Window
(defwindow calendar
  :monitor 0
  :windowtype "dock"
  :stacking "overlay"
  :geometry
    (geometry
      :x "4px"
      :y "36px"
      :width "14%"
      :height "18%"
      :anchor "top left")
(cal))
